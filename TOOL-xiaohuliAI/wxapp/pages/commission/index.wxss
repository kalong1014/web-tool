page {
    background: #f7f7f7;
}

.page {
    height: auto;
    min-height: 100%;
    overflow-x: hidden;
    overflow-y: scroll;
    padding: 20rpx;
}

.fui-list {
    overflow: hidden;
    padding: 30rpx 24rpx;
    transition-duration: .3s;
}

.fui-list,.fui-list-media {
    align-items: center;
    display: flex;
    position: relative;
}

.fui-list-media {
    box-sizing: border-box;
    color: #333;
    flex-shrink: 0;
    flex-wrap: nowrap;
    margin-right: 20rpx;
}

.fui-list-inner {
    box-sizing: border-box;
    display: block;
    flex: 1;
}

.fui-block-group,.fui-list-inner {
    overflow: hidden;
    position: relative;
}

.fui-block-group {
    background: #fff;
    border-radius: 16rpx;
    height: auto;
    margin-top: 24rpx;
}

.fui-block-group .fui-block-child {
    background: #fff;
    float: left;
    height: auto;
    padding: 16rpx 0;
    position: relative;
    transition: background-color .3s;
    width: 33.33%;
}

.fui-block-group .fui-block-child .icon {
    font-size: 56rpx;
    height: 108rpx;
    line-height: 60rpx;
    padding-top: 40rpx;
    text-align: center;
}

.fui-cell-group {
    background-color: #fff;
    font-size: 32rpx;
    line-height: 1.4;
    margin-top: 24rpx;
    overflow: hidden;
    position: relative;
}

.fui-cell-group .fui-cell {
    align-items: center;
    border-bottom: 1px solid #f8f8f8;
    display: flex;
    padding: 24rpx;
    position: relative;
}

.fui-cell-group .fui-cell:last-child {
    border-bottom: none;
}

.fui-cell-group .fui-cell .icon {
    height: 32rpx;
    margin-right: 12rpx;
    width: 32rpx;
}

.fui-cell-group .fui-cell .fui-cell-text {
    color: #444;
    flex: 1;
    font-size: 28rpx;
}

.fui-cell-group .fui-cell .icon-r {
    height: 26rpx;
    margin-right: 8rpx;
    width: 26rpx;
}

.commission .user-info {
    background: #04babe;
    border-radius: 16rpx;
    padding: 8rpx 0;
    width: 100%;
}

.commission .user-info image {
    background: #f7f7f7;
    border-radius: 50%;
    height: 120rpx;
    width: 120rpx;
}

.commission .fui-list-media {
    margin-right: 28rpx;
}

.commission .user-info .title {
    color: #fff;
    font-size: 30rpx;
    font-weight: 700;
    line-height: 40rpx;
    margin-bottom: 10rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.commission .user-info .subtitle {
    border: 1rpx solid #fff;
    border-radius: 30rpx;
    box-sizing: border-box;
    display: inline-block;
    line-height: 36rpx;
    max-width: 100%;
    padding: 0 16rpx;
    width: auto;
}

.commission .user-info .subtitle,.commission .user-info .text {
    color: #fff;
    font-size: 22rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.commission .user-info .text {
    line-height: 40rpx;
}

.commission .userblock {
    border-radius: 16rpx;
    margin-top: 24rpx;
    overflow: hidden;
}

.commission .userblock .line {
    padding: 15rpx 0;
}

.commission .userblock .line.total {
    background: #fff;
    box-sizing: border-box;
    height: 166rpx;
    padding: 32rpx 24rpx;
}

.commission .userblock .line.total .title {
    color: #333;
    font-size: 24rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.commission .userblock .line.total .num {
    color: #04babe;
    font-size: 44rpx;
    font-weight: 700;
    line-height: 1.5;
}

.commission .userblock .line.able {
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    align-items: center;
    background: #fff;
    display: -ms-flexbox;
    display: flex;
    font-size: 27rpx;
    height: 166rpx;
    justify-content: space-between;
    padding: 0 24rpx;
    position: relative;
}

.commission .userblock .line.able .num {
    color: #04babe;
    font-size: 44rpx;
    font-weight: 700;
    line-height: 1.5;
}

.commission .userblock .line .text {
    padding-right: 230rpx;
}

.commission .userblock .line .btn {
    border: 1px solid #fff;
    border-radius: 56rpx;
    font-size: 26rpx;
    height: 56rpx;
    line-height: 54rpx;
    margin-right: 0;
    padding: 0 24rpx;
    width: auto;
}

.btn-warning {
    background: #04babe;
    border: 1px solid #04babe;
    color: #fff;
}

.commission .userblock .line.able:after {
    border-top: 1px solid #ebebeb;
    color: #d9d9d9;
    content: " ";
    height: 1px;
    left: 20rpx;
    position: absolute;
    right: 20rpx;
    top: -1px;
    transform: scaleY(.5);
    transform-origin: 0 0;
}

.commission .fui-block-group:after {
    border-top: 1px solid #ebebeb;
    color: #d9d9d9;
    content: " ";
    height: 1px;
    left: 20rpx;
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: scaleY(.5);
    transform-origin: 0 0;
}

.commission .fui-block-group:before {
    border-left: 1px solid #ebebeb;
    bottom: 20rpx;
    color: #d9d9d9;
    content: " ";
    left: 50%;
    position: absolute;
    top: 20rpx;
    transform: scaleX(.5);
    transform-origin: 0 0;
    width: 1px;
    z-index: 2;
}

.commission .fui-block-group .fui-block-child {
    -webkit-box-align: center;
    -ms-flex-align: center;
    -webkit-box-pack: start;
    -ms-flex-pack: start;
    align-items: center;
    box-sizing: border-box;
    display: -ms-flexbox;
    display: flex;
    height: 148rpx;
    justify-content: flex-start;
    padding-left: 24rpx;
    width: 50%;
}

.fui-block-group .fui-block-child::after,.fui-block-group .fui-block-child::before {
    display: none;
}

.commission .fui-block-group .fui-block-child .icon {
    height: auto;
    padding: 0;
}

.fui-block-group .fui-block-child .icon i {
    font-size: 52rpx;
}

.commission .fui-block-group .fui-block-child .icon image {
    height: 70rpx;
    width: 70rpx;
}

.commission .fui-block-group .fui-block-child .text {
    color: #999;
    font-size: 24rpx;
    height: auto;
    line-height: 1.5;
    margin-left: 24rpx;
    text-align: left;
}

.commission .fui-block-group .fui-block-child .title {
    color: #333;
    font-size: 28rpx;
    height: auto;
    line-height: normal;
    text-align: left;
}

.commission .fui-block-group .fui-block-child .text text,.edtion {
    color: #999;
}

.edtion {
    bottom: 100rpx;
    font-size: 28rpx;
    padding: 20rpx 0;
    position: fixed;
    text-align: center;
    width: 100%;
}

.li-wrap {
    background: #fff;
    flex-direction: column;
    margin-bottom: 2rpx;
    padding-left: 30rpx;
}

.li-wrap,.li-wrap .list {
    align-items: center;
    display: flex;
}

.li-wrap .list {
    border-bottom: 1rpx solid #f1f1f1;
    box-sizing: border-box;
    height: 100rpx;
    justify-content: space-between;
    padding-right: 30rpx;
    width: 100%;
}

.li-wrap .list:last-child {
    border-bottom: none;
}

.li-wrap .list .list-l {
    align-items: center;
    display: flex;
    justify-content: flex-start;
}

.li-wrap .list .list-l .icon {
    height: 40rpx;
    margin-right: 20rpx;
    width: 40rpx;
}

.li-wrap .list .list-l .icon .img {
    height: 40rpx;
    width: 40rpx;
}

.li-wrap .list .list-l .text {
    color: #333;
    font-size: 32rpx;
}

.li-wrap .list .arrow,.li-wrap .list .arrow image {
    height: 26rpx;
    width: 26rpx;
}

.footer {
    font-size: 26rpx;
    margin: 60rpx 0;
    text-align: center;
    width: 100%;
}

.footer navigator {
    display: inline;
}